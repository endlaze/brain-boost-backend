{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models",
      "../models/company-info-model",
      "../models/faq-model",
      "../models/app-user-model",
      "../models/stock-roles-model",
      "../models/reminders-model",
      "../models/stock-sub-roles-model"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db"
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "reminders": {
    "dataSource": "postgreDS",
    "public": true
  },
  "stock_subroles": {
    "dataSource": "postgreDS",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true
        }
      }
    }
  }
}
